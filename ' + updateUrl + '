
   
<script>
document.addEventListener("DOMContentLoaded", function () {
    console.log("Breadcrumbs script loaded");

    function hidePhpBreadcrumbs() {
        const jsBreadcrumbs = document.querySelector(".breadcrumbs-wrapper");
        const phpBreadcrumb = document.querySelector("nav.php-breadcrumb");
        const tzExp = document.querySelector(".landingTz-main-screen");
        if (jsBreadcrumbs && phpBreadcrumb) {
            console.log("Hiding PHP breadcrumbs: JS breadcrumbs (.breadcrumbs-wrapper) detected");
            phpBreadcrumb.style.display = "none";
            return true;
        } else if(phpBreadcrumb && tzExp){
            phpBreadcrumb.style.display = "none";
        }
        return false;
    }

    if (hidePhpBreadcrumbs()) {
        return;
    }

    const observer = new MutationObserver(function (mutations, obs) {
        if (hidePhpBreadcrumbs()) {
            obs.disconnect();
        }
    });

    observer.observe(document.body, {
        childList: true,
        subtree: true
    });
});
</script>  <!doctype html>
